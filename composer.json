    "require": {
        "php": ">=5.6.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "aura/di": "^1.0",
        "illuminate/container": "^4.0 || ^5.0",
        "mnapoli/php-di": "^3.0",
        "nette/nette": "^2.1",
        "phpunit/phpunit": "^5.7 || ^6.0",
        "pimple/pimple": "^3.0",
        "symfony/dependency-injection": "^2.1 || ^3.0",
        "zendframework/zend-di": "^2.0",
        "zendframework/zend-servicemanager": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Acclimate\\Container\\": "src/"
